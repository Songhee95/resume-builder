    <body>
        <main class="columns">
            <!-- <section class="column"></section> -->
            <section class="column is-half is-offset-one-quarter formContainer">
                <section>
                    <form class="field">
                        <label class="label">Resume Name</label>
                        <!-- <h1><label>Resume Name</label></h1> -->
                        <div class="control">
                            <input type="text" id="resumeName" size="30" {{resumeName}}><br><br>
                        </div>
                    </form>
                </section>
                <section class="objectiveContainer">
                    <form id="objectiveForm">
                        <label class="label">Objective Statement</label>
                        <textarea type="text" class="textarea" id="objective" name="objective" rows="3" cols="20" placeholder="What is your objective?">{{objective}}</textarea><br>
                    </form>
                </section>
                <!-- SCHOOL A -->
                {{#if isSchool}}
                <section class="education" data-index="1">
                    <form id="educationForm" class="field">
                        <label class="label">Education</label>
                        <label class="educationEntries">School</label>
                        <div class="control">
                            <input type="text" class="school" id="school" size="40" {{schoolA}}><br>
                        </div>
                        <label class="educationEntries">Degree</label>
                        <div class="control">
                            <input type="text" class="degree" name="degree" size="40" {{degreeA}}><br>
                        </div>
                        <div class="field is-horizontal columns">
                            <div class="column">
                                <label class="educationEntries">Start Date</label>
                                <div class="control">
                                    <input type="text" class="startDate" id="startDate" size="40" {{startDateA}}}>
                                </div>
                            </div>
                            <div class="column">
                                <label class="educationEntries">End Date</label>
                                <div class="control">
                                    <input type="text" class="endDate" id="endDate" size="40" {{endDateA}}><br>
                                </div>
                            </div>
                        </div>
                        <label class="educationEntries">Program</label>
                        <div class="control">
                            <input type="text" class="program" id="program" size="40" {{programA}}><br>
                        </div>
                    </form>
                </section>
                {{/if}}
                <!-- SCHOOL B -->
                {{#if isSchoolB}}
                <section class="educationB" data-index="1">
                    <form id="educationForm" class="field">
                        <label class="label">Education</label>
                        <label class="educationEntries">School</label>
                        <div class="control">
                            <input type="text" class="school" id="schoolB" size="40" {{schoolB}}><br>
                        </div>
                        <label class="educationEntries">Degree</label>
                        <div class="control">
                            <input type="text" class="degree" id="degreeB" size="40" {{degreeB}}><br>
                        </div>
                        <div class="field is-horizontal columns">
                            <div class="column">
                                <label class="educationEntries">Start Date</label>
                                <div class="control">
                                    <input type="text" class="schoolStartDate" id="startDateB" size="40" {{startDateB}}}>
                                </div>
                            </div>
                            <div class="column">
                                <label class="educationEntries">End Date</label>
                                <div class="control">
                                    <input type="text" class="schoolEndDate" id="endDateB" size="40" {{endDateB}}><br>
                                </div>
                            </div>
                        </div>
                        <label class="educationEntries">Program</label>
                        <div class="control">
                            <input type="text" class="program" id="programB" size="40" {{programB}}><br>
                        </div>
                    </form>
                </section>
                {{/if}}
                {{#if isSchoolC}}
                <section class="educationB" data-index="1">
                    <form id="educationForm" class="field">
                        <label class="label">Education</label>
                        <label class="educationEntries">School</label>
                        <div class="control">
                            <input type="text" class="school" id="schoolC" size="40" {{schoolB}}><br>
                        </div>
                        <label class="educationEntries">Degree</label>
                        <div class="control">
                            <input type="text" class="degree" id="degreeC" size="40" {{degreeB}}><br>
                        </div>
                        <div class="field is-horizontal columns">
                            <div class="column">
                                <label class="educationEntries">Start Date</label>
                                <div class="control">
                                    <input type="text" class="schoolStartDate" id="startDateC" size="40" {{startDateB}}}>
                                </div>
                            </div>
                            <div class="column">
                                <label class="educationEntries">End Date</label>
                                <div class="control">
                                    <input type="text" class="schoolEndDate" id="endDateC" size="40" {{endDateB}}><br>
                                </div>
                            </div>
                        </div>
                        <label class="educationEntries">Program</label>
                        <div class="control">
                            <input type="text" class="program" id="programC" size="40" {{programB}}><br>
                        </div>
                    </form>
                </section>
                {{/if}}
                <button type="button" id="addEducation">Add Education</button><br><br>
                {{#if isEmployer}}
                <section class="experience" data-index="1">
                    <form id="experienceForm">
                        <label class="label">Experience</label>
                        <label class="experienceEntries">Employer</label>
                        <div class="control">
                            <input type="text" class="employer" id="employer" size="40" {{employerA}}><br>
                        </div>

                        <label class="experienceEntries">Job Title</label>
                        <div class="control">
                            <input type="text" class="title" id="jobTitle" size="40" {{titleA}}><br>
                        </div>
                        <div class="field is-horizontal columns">
                            <div class="column">
                                <label class="experienceEntries">Start Date</label>
                                <div class="control">
                                    <input type="text" class="jobStart" id="jobStartDate" size="40" {{jobStartA}}>
                                </div>
                            </div>
                            <div class="column">
                                <label class="experienceEntries">End Date</label>
                                <div class="control">
                                    <input type="text" class="jobEnd" id="jobEndDate" size="40" {{jobEndA}}><br>
                                </div>
                            </div>
                        </div>
                        

                        <label class="experienceEntries">Description</label>
                        <div class="control">
                            <textarea type="text" class="textarea description" id="description" rows="5" cols="50">{{descriptionA}}</textarea><br>
                        </div>
                    </form>
                    
                </section>
                {{/if}}
                {{#if isEmployerB}}
                <section class="experienceB" data-index="1">
                    <form id="experienceForm">
                        <label class="label">Experience</label>
                        <label class="experienceEntries">Employer</label>
                        <div class="control">
                            <input type="text" class="employer" id="employerB" size="40" {{employerB}}><br>
                        </div>

                        <label class="experienceEntries">Job Title</label>
                        <div class="control">
                            <input type="text" class="jobTitle" id="titleB" size="40" {{titleB}}><br>
                        </div>
                        <div class="field is-horizontal columns">
                            <div class="column">
                                <label class="experienceEntries">Start Date</label>
                                <div class="control">
                                    <input type="text" class="jobStartDate" id="jobStartB" size="40" {{jobStartB}}>
                                </div>
                            </div>
                            <div class="column">
                                <label class="experienceEntries">End Date</label>
                                <div class="control">
                                    <input type="text" class="jobEndDate" id="jobEndB" size="40" {{jobEndB}}><br>
                                </div>
                            </div>
                        </div>
                    
                        <label class="experienceEntries">Description</label>
                        <div class="control">
                            <textarea type="text" class="textarea description" id="descriptionB" rows="5" cols="50">{{descriptionB}}</textarea><br>
                        </div>
                    </form>
                    
                </section>
                {{/if}}
                {{#if isEmployerC}}
                <section class="experienceC" data-index="1">
                    <form id="experienceForm">
                        <label class="label">Experience</label>
                        <label class="experienceEntries">Employer</label>
                        <div class="control">
                            <input type="text" class="employer" id="employerC" size="40" {{employerC}}><br>
                        </div>

                        <label class="experienceEntries">Job Title</label>
                        <div class="control">
                            <input type="text" class="jobTitle" id="titleC" size="40" {{titleC}}><br>
                        </div>
                        <div class="field is-horizontal columns">
                            <div class="column">
                                <label class="experienceEntries">Start Date</label>
                                <div class="control">
                                    <input type="text" class="jobStartDate" id="jobStartC" size="40" {{jobStartC}}>
                                </div>
                            </div>
                            <div class="column">
                                <label class="experienceEntries">End Date</label>
                                <div class="control">
                                    <input type="text" class="jobEndDate" id="jobEndC" size="40" {{jobEndC}}><br>
                                </div>
                            </div>
                        </div>
                    
                        <label class="experienceEntries">Description</label>
                        <div class="control">
                            <textarea type="text" class="textarea description" id="descriptionC" rows="5" cols="50">{{descriptionC}}</textarea><br>
                        </div>
                    </form>
                </section>
                {{/if}}
                <button type="button" id="addExperience">Add Experience</button><br><br>
                {{#if isProject}}
                <section class="project" data-index="1">
                    <form id="projectForm">
                        <label class="label">Projects</label>
                        <label class="projectEntries">Project Title</label>
                        <input type="text" class="projectTitle" id="project" size="40" {{projectA}}><br><br>

                        <label>Project Description</label>
                        <textarea type="text" class="textarea" class="summary" id="summary" rows="5" cols="50">{{summaryA}}</textarea><br>
                    </form>
                </section>
                {{/if}}
                {{#if isProjectB}}
                <section class="projectB" data-index="1">
                    <form id="projectForm">
                        <label class="label">Projects</label>
                        <label class="projectEntries">Project Title</label>
                        <input type="text" class="projectTitle" id="projectB" size="40" {{projectB}}><br><br>

                        <label>Project Description</label>
                        <textarea type="text" class="textarea" class="projectDescription" id="summaryB" rows="5" cols="50">{{summaryB}}</textarea><br>
                    </form>
                </section>
                {{/if}}
                {{#if isProjectC}}
                <section class="projectC" data-index="1">
                    <form id="projectForm">
                        <label class="label">Projects</label>
                        <label class="projectEntries">Project Title</label>
                        <input type="text" class="projectTitle" id="projectC" size="40" {{projectC}}><br><br>

                        <label>Project Description</label>
                        <textarea type="text" class="textarea" class="projectDescription" id="summaryC" rows="5" cols="50">{{summaryC}}</textarea><br>
                    </form>
                </section>
                {{/if}}
                <button type="button" id="addProject">Add Another Project</button><br><br>
                <section class="skills">
                    <form id="skillsform">
                        <label class="label">Skills</label>
                        <textarea type="text" class="textarea" id="skills" name="skills" rows="5" cols="30">{{skills}}</textarea>
                    </form>
                </section>
                <section class="awards">
                    <form id="awardsForm">
                        <label class="label">Awards</label>
                        <textarea type="text" class="textarea" id="awards" name="awards" rows="5" cols="30">{{awards}}</textarea>
                    </form>
                </section>
                
                <!-- <button type="button" id="saveButton">Save Resume</button> -->
                <div class="field is-grouped is-grouped-right">
                    <p class="control">
                        <a class="button is-primary" id="saveButton">
                            Save as New Resume
                        </a>
                    </p>
                    <p>
                        <a class="button is-primary" id="updateButton">
                            Update Current
                        </a>
                    </p>
                    <p class="control">
                        <a class="button is-light">
                            Cancel
                        </a>
                    </p>
                </div>
            </section>
            <!-- <section class="column"></section> -->
        </main>
        
        <script src="../public/assets/js/formEdit.js"></script>
    </body>
<!-- </html> -->