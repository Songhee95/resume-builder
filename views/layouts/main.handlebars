<script src="./js/index.js" type="text/javascript"></script>
<script>
    const user = JSON.parse(sessionStorage.getItem('userId'))
    if (user) {
        $(".navbar-before-login")
            .removeClass('visible')
            .addClass('is-hidden')
        $('.navbar-after-login')
            .removeClass('is-hidden')
            .addClass('visible')
    } else {
        $(".navbar-before-login")
            .removeClass('is-hidden')
            .addClass('visible')
        $('.navbar-after-login')
            .removeClass('visible')
            .addClass('is-hidden')

    }

    $(".logout__button").click(function () {
        sessionStorage.setItem("userId", null);
    });
</script>
